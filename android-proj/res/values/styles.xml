<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="Theme.Sherlock.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
        <item name="img_basket">@drawable/ic_holo_light_action_basket</item>
        <item name="img_in_basket">@drawable/ic_holo_light_action_drop</item>
        <item name="img_out_basket">@drawable/ic_holo_light_action_extract</item>
        <item name="img_filter">@drawable/ic_holo_light_action_filter</item>
        <item name="img_browser">@drawable/ic_holo_light_action_globe</item>
        <item name="img_refresh">@drawable/ic_holo_light_action_reload</item>
        <item name="img_search">@drawable/ic_holo_light_action_search</item>
        <item name="img_sort">@drawable/ic_holo_light_action_sort</item>
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

</resources>