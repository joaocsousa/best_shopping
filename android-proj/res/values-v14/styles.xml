<resources>

    <!--
        Base application theme for API 14+. This theme completely replaces
        AppBaseTheme from BOTH res/values/styles.xml and
        res/values-v11/styles.xml on API 14+ devices.
    -->
    <style name="AppBaseTheme" parent="Theme.Sherlock.Light.DarkActionBar">
        <!-- API 14 theme customizations can go here. -->
        <item name="img_basket">@drawable/ic_holo_dark_action_basket</item>
        <item name="img_in_basket">@drawable/ic_holo_dark_action_drop</item>
        <item name="img_out_basket">@drawable/ic_holo_dark_action_extract</item>
        <item name="img_filter">@drawable/ic_holo_dark_action_filter</item>
        <item name="img_browser">@drawable/ic_holo_dark_action_globe</item>
        <item name="img_refresh">@drawable/ic_holo_dark_action_reload</item>
        <item name="img_search">@drawable/ic_holo_dark_action_search</item>
        <item name="img_sort">@drawable/ic_holo_dark_action_sort</item>
    </style>

</resources>